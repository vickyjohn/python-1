#include<bits/stdc++.h>
using namespace std;
 
int calculate(int n, int power)
{
    int sum = 0;
    int bp = (int)pow(n, power);
    while (bp != 0) {
        int d = bp % 10;
        sum += d;
        bp /= 10;
    }
    return sum;
}
 
// Driver Code
int main()
{
    int n = 5;
    int power = 4;
    cout << calculate(n, power);
}
